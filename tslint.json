{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rulesDirectory": [],
    "rules": {
        "quotemark": [true, "single"],
        "no-console": false,
        "ordered-imports": false,
        "max-line-length": [true, {"limit": 120, "ignore-pattern": "^import |^export {(.*?)}"}]
    }
}